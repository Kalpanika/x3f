if [ -z $1 ]; then
    # TODO: This is a hack, not wanting to spend too much time on this
    # If someone runs osx or windows, we have to fix it, maybe ...
    echo No system given
    echo Assumimg linux
    SYSTEM=linux
else
    SYSTEM=$1
fi

export LD_LIBRARY_PATH=$PWD/deps/lib/$SYSTEM/opencv/lib

echo LD_LIBRARY_PATH=$LD_LIBRARY_PATH
